const run = require('./function');
const path = require('path');

const {
  faaspath,
  uuid
} = require('../core/config');

module.exports = (handler) => (req, res) => {
  

  run(path.join(faaspath, uuid, handler), data, (result) => {
    res.status(result.statusCode || 200).send(result);
  })
}
